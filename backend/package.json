  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/src/index.js",
    "start:dev": "tsc-watch --onSuccess \"node dist/src/index\"",
    "db:generate": "drizzle-kit generate --config ./src/drizzle.config.ts",
    "db:push": "drizzle-kit push --config ./src/drizzle.config.ts",
    "db:migrate": "tsx ./src/drizzle/migrate.ts",
    "db:deletemigrations": "drizzle-kit drop --config ./src/drizzle.config.ts",
    "db:generateTest": "drizzle-kit generate --config ./src/test/drizzle.test.config.ts",
    "test": "tsc && environment=test jest --forceExit --silent --verbose --runInBand --json --outputFile=test-results.json",
    "test:debug": "environment=test jest --forceExit --verbose --runInBand"
  },
