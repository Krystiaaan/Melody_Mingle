    "@types/uuid": "^9.0.8",
    "drizzle-kit": "^0.22.6",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.2.0",
    "tsx": "^4.15.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
